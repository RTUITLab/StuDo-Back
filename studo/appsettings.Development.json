{
  "$schema": "./appsettings.Secret.Schema.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "LogsOptions": {
    "SecretKey": "logsOptions__SecretKey"
  },
  "JwtOptions": {
    "SecretKey": "jwOptions__SecretKey",
    "Audience": "http://localhost:4200",
    "Issuer": "localhost",
    "RefreshTokenLifeTime": "10:00:00",
    "AccessTokenLifeTime": "00:05:00"
  },
  "FillDbOptions": {
    "Firstname": "test",
    "Surname": "testsur",
    "Email": "test@gmail.com",
    "Password": "123456"
  },
  "USE_DEBUG_EMAIL_SENDER": true,
  "FILL_DB": true,
  "MIGRATE": true,
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=studoDB;Trusted_Connection=True;",
    "PostgreSQL": "Server=localhost;Port=5432;Database=StudoDB;User Id=postgres;Password=password"
  }
}
